	/* 团本列表 */
.dungeon-list.data-v-176a9427 {
		display: flex;
		flex-direction: row;
		align-items: center;
}
.dungeon-item.data-v-176a9427 {
		position: relative;
		background-size: 100% 100%;
		/* margin: 15rpx 10rpx; */
		width: 190rpx;
		height: 120rpx;
		/* flex: 1; */
		/* background: #fff; */
		/* padding: 20rpx; */
		border-radius: 10rpx;
		color: #888;
		font-size: 24rpx;

		display: flex;
		align-items: center;
		justify-content: center;
		flex-direction: column;
		margin-right: 20rpx;
}
.dungeon-item.data-v-176a9427:last-child {
		margin-right: 0;
}
.dungeon-label.data-v-176a9427 {
		position: absolute;
		right: 0rpx;
		bottom: 1rpx;
		padding: 10rpx 10rpx;
		font-size: 26rpx;
		background-color: rgba(0, 0, 0, 0.8);
		color: #fff;
		border-radius: 10rpx 0rpx 10rpx 0rpx;
}

	/* 内部 Tab */
.inner-tab.data-v-176a9427 {
		flex-direction: row;
		display: flex;
		/* border: 1rpx solid #eee; */
		border-radius: 10rpx;
		overflow: hidden;
		height: 80rpx;
		margin-top: 24rpx;
		margin-bottom: 24rpx;
		box-shadow: 0 2rpx 8rpx rgba(0, 0, 0, 0.1);
		/* 阴影效果 */
}
.leilong-btn.data-v-176a9427 {
		display: flex;
		align-items: center;
		justify-content: center;
		height: 80rpx;
		border-radius: 10rpx;
		background: #fff;
		color: #3366ff;
		font-weight: 500;
		border: 1rpx solid #3366ff;
		box-shadow: 0 2rpx 8rpx rgba(0, 0, 0, 0.1);
		/* 阴影效果 */
		margin-bottom: 24rpx;
}
.inner-tab-item.data-v-176a9427 {
		flex: 1;
		text-align: center;
		padding: 20rpx 0;
		color: #666;
		display: flex;
		align-items: center;
		justify-content: center;
}

	/* .inner-tab-item.active {
  color: #409eff;
  background: #f6f8ff;
} */
.inner-tab-item text.data-v-176a9427 {
		display: inline-block;
		position: relative;
}
.inner-tab-item.active text.data-v-176a9427 {
		color: #007aff;
		font-weight: bold;
		font-size: 30rpx;
}
.inner-tab-item.active text.data-v-176a9427::after {
		content: '';
		position: absolute;
		bottom: -10rpx;
		/* 距离文字底部的距离 */
		left: 0;
		width: 100%;
		height: 5rpx;
		background: linear-gradient(90deg, #366fff 0%, #5da0fc 100%);
		border-radius: 2rpx;
}
.right-tab-icon.data-v-176a9427 {
		width: 36rpx;
		height: 36rpx;
		margin-right: 12rpx;
}
.current-role.data-v-176a9427 {
		display: flex;
		justify-content: space-between;
		align-items: center;
		margin-bottom: 15rpx;
}
.role-name.data-v-176a9427 {
		margin-left: 20rpx;
		font-size: 30rpx;
}
.role-seller.data-v-176a9427 {
		color: #409eff;
}
